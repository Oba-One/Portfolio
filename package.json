{
  "name": "portfolio",
  "version": "1.0.0",
  "homepage": "https://afolabi.info",
  "description": "Portfolio site for Afolabi Aiyeloja",
  "repository": "https://github.com/Oba-One/portfolio.git",
  "author": "Afolabi Aiyeloja <afo@wefa.app>",
  "scripts": {
    "dev": "next dev",
    "storybook": "start-storybook -p 9009 -h localhost",
    "build": "next build",
    "build:storybook": "build-storybook -o build-storybook && node scripts/draco-storybook",
    "start": "next start",
    "test": "echo 'write some tests'",
    "lint": "next lint",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,md,mdx}'",
    "optimize:assets": "zsh scripts/optimize-assets.sh",
    "optimize:resize": "npx @squoosh/cli -s '-mobile' --webp auto --resize \"{'enabled':true,'width':'25%','height':'25%','method':'lanczos3'}\" src/assets/*.webp"
  },
  "dependencies": {
    "cors": "2.8.5",
    "framer-motion": "^7.1.0",
    "next": "^12.2.5",
    "nodemailer": "6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.146.0",
    "three-stdlib": "^2.21.8",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@squoosh/cli": "^0.7.3",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-controls": "^6.5.10",
    "@storybook/addon-toolbars": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@svgr/webpack": "^6.3.1",
    "@types/cors": "^2.8.13",
    "@types/node": "18.7.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "esbuild": "^0.17.10",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "8.6.0",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "mdx-bundler": "^9.2.1",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.58.3",
    "storybook": "^6.5.10",
    "storybook-addon-next": "^1.6.7",
    "typescript": "4.9.5"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 90,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "next",
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "prefer-const": "error",
      "no-unused-vars": "warn",
      "import/no-anonymous-default-export": "off",
      "react/display-name": "off",
      "@next/next/no-img-element": "off"
    }
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=18.0.0"
  }
}
