    "@storybook/addon-controls": "^6.5.10",
    "@storybook/addon-toolbars": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@svgr/webpack": "^6.3.1",
    "esbuild": "^0.15.2",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "fs-extra": "^10.1.0",
    "globby": "^13.1.2",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "puppeteer": "^16.1.0",
    "reading-time": "^1.5.0",
    "rehype-img-size": "^1.0.1",
    "rehype-preset-minify": "^6.0.0",
    "rehype-slug": "^5.0.1",
    "sass": "^1.58.3",
    "storybook": "^6.5.10",
    "storybook-addon-next": "^1.6.7"
  },
  "dependencies": {
    "framer-motion": "^7.1.0",
    "mdx-bundler": "^9.2.1",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.149.0",
    "three-stdlib": "^2.21.8"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:storybook": "build-storybook -o build-storybook && node scripts/draco-storybook",
    "deploy:storybook": "aws s3 sync --delete build-storybook s3://hamish-portfolio-storybook",
    "start": "next start",
    "test": "echo 'write some tests'",
    "storybook": "start-storybook -p 9009 -h localhost"
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 90,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "next/core-web-vitals"
    ],
    "rules": {
      "semi": "error",
      "no-unused-vars": "warn",
      "import/no-anonymous-default-export": "off",
      "react/display-name": "off",
      "@next/next/no-img-element": "off"
    }
  },
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=18.0.0"
  }
}
